@model List<TripTrack.Models.Destination>
   


<h1>@ViewBag.Title</h1>


@if (Model.Count == 0)
{
    <p>No destinations yet!</p>
}

<p><a asp-controller="Destination" asp-action="Add">AddDestination</a></p>

<title>Destination</title>

<table class="table">

   
        
    <tr>
        <td>Trip</td>
        <td>Date</td>
        <td>Location</td>
        <td>Airline</td>
        <td>Hotel</td>
        <td>RentaCar</td>
        <td>Code</td>
    </tr>

    @foreach (var destination in Model)

    {
<tr>
    <td>@destination.Trip</td>
    <td>@destination.Date</td>
    <td>@destination.Location</td>



    <td>@destination.Airline</td>
    <td>@destination.Hotel</td>
    <td>@destination.RentaCar</td>
    <td>@destination.Code</td>


</tr>
    }
</table>


       
    
   
